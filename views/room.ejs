    <title>Codefest Project</title>
    <style>
        /*
        generates a 300 x 300 square video
        */
        #video-grid{
            display: grid;
            grid-template-columns: repeat(auto-fill, 300px);
            grid-gap: 10px;
            grid-auto-rows: 300px;
            padding: 5px;
            text-align: center;
            font-size: 25px;
        }
        video{
            width: 100%;
            height: 100%;
            object-fit: cover;
        }
    </style>
</head>
<body>
    <h1>Codefest Project WPI</h1>
    <p></p>
    <div id = "video-grid"></div>
</body>
